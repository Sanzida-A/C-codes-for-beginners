#include<stdio.h>

int main()
{
    char x[100],y[100],z[100];
    int i,j;
    printf("Enter The First Name : ");
    gets(x);
    printf("\nEnter The Last Name : ");
    gets(y);

    for(i=0;x[i]!='\0';i++)
    {
        z[i]=x[i];
    }

    z[i]=' ';    i++;
    for(j=0;y[j]!='\0';j++)
    {

        z[i]=y[j];
            i++;
    }
    z[i]='\0';
    printf("\nFull Name:");
    puts(z);

    return 0;
}
